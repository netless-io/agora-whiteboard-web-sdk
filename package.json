{
  "name": "fastboard-monorepo",
  "private": "true",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .ts,.tsx . && prettier --check .",
    "check": "pnpm -r check",
    "dev": "pnpm -F=fastboard dev",
    "dev:react": "pnpm -F=fastboard-react dev",
    "dev:ui": "pnpm -F=fastboard-ui dev",
    "build": "pnpm -r build",
    "build:ci": "pnpm --aggregate-output -r build",
    "cleanup": "pnpm -r cleanup",
    "release": "esbuild-dev scripts/release.ts"
  },
  "devDependencies": {
    "@hyrious/esbuild-dev": "^0.8.5",
    "@hyrious/esbuild-plugin-svelte": "^0.1.6",
    "@hyrious/rimraf": "^0.1.0",
    "@netless/synced-store": "^2.0.7",
    "@netless/window-manager": "1.0.0-canary.63",
    "@sveltejs/vite-plugin-svelte": "1.0.8",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "eslint-define-config": "^1.7.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.2.12",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "semver": "^7.3.7",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "white-web-sdk": "^2.16.36"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom"
      ]
    }
  }
}

{
  "name": "fastboard-monorepo",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .ts,.tsx . && prettier --check .",
    "check": "pnpm -r check",
    "dev": "pnpm -F=fastboard dev",
    "dev:react": "pnpm -F=fastboard-react dev",
    "dev:ui": "pnpm -F=fastboard-ui dev",
    "build": "pnpm -r build",
    "build:ci": "pnpm --aggregate-output -r build",
    "cleanup": "pnpm -r cleanup",
    "release": "esbuild-dev scripts/release.ts"
  },
  "devDependencies": {
    "@hyrious/esbuild-dev": "^0.10.2",
    "@hyrious/esbuild-plugin-svelte": "^0.1.8",
    "@hyrious/rimraf": "^0.1.0",
    "@netless/synced-store": "^2.0.7",
    "@netless/window-manager": "1.0.0-canary.72",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "esbuild": "^0.18.2",
    "eslint": "^8.42.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.2.12",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "semver": "^7.5.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "white-web-sdk": "^2.16.42"
  },
  "pnpm": {
    "overrides": {
      "tsup>esbuild": "^0.18.2"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom"
      ]
    }
  }
}

{
  "name": "@netless/fastboard",
  "version": "0.0.11",
  "description": "An open sourced sdk based on white-web-sdk.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.cjs.js"
      },
      "default": "./dist/index.es.js",
      "types": "./src/index.ts"
    },
    "./preact": {
      "node": {
        "import": "./dist/preact.es.js",
        "require": "./dist/preact.cjs.js"
      },
      "default": "./dist/preact.es.js",
      "types": "./src/preact.tsx"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "esbuild-dev scripts/prepublish.ts",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx . && tsc",
    "build": "vite build",
    "build:dev": "vite build --mode development"
  },
  "contributors": [
    {
      "name": "hyrious",
      "email": "hyrious@outlook.com"
    },
    {
      "name": "l1shen",
      "email": "lishen1635@gmail.com"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "@netless/app-slide": "*",
    "@netless/window-manager": "*",
    "white-web-sdk": "*"
  },
  "peerDependenciesMeta": {
    "@netless/app-slide": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "framer-motion": "^5.6.0",
    "i18next": "^21.6.6",
    "preact": "^10.6.4",
    "rc-slider": "^9.7.5",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@netless/app-slide": "^0.0.55",
    "@netless/window-manager": "^0.4.0-canary.16",
    "@tippyjs/react": "^4.2.6",
    "@types/react": "^17.0.38",
    "@vitejs/plugin-react": "^1.1.4"
  }
}
